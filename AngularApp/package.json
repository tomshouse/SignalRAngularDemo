{
  "name": "my-first-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "start": "npm run ng serve --host=127.0.0.1 --sourcemap",
    "build": "npm run ng build",
    "build-droid": "npm run ng build && del /Q ..\\Droid\\Assets\\Content\\* && xcopy dist\\*.* ..\\Droid\\Assets\\Content\\*.* /S /C /Y && del /Q ..\\iOS\\Content\\* && copy dist\\* ..\\iOS\\Content && del /Q ..\\UWP\\Content\\* && copy dist\\* ..\\UWP\\Content",
    "build-release": "npm run ng build --aot=true --prod=true --optimization=true && del /Q ..\\Droid\\Assets\\Content\\* && copy dist\\* ..\\Droid\\Assets\\Content && del /Q ..\\iOS\\Content\\* && copy dist\\* ..\\iOS\\Content && del /Q ..\\UWP\\Content\\* && copy dist\\* ..\\UWP\\Content",
    "clean": "npm install",
    "test": "npm run ng test",
    "lint": "npm run ng lint",
    "e2e": "npm run ng e2e"
  },
